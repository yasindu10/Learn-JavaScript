	<title>JS Array filter</title>
</head>
<body>


<p id="demo"></p>
	

<script>
// array
let mathsMarks = [50, 79, 80, 66, 90, 95];

// filter
let filter = mathsMarks.filter(myFunction);

document.getElementById('demo').innerHTML = filter;

function myFunction(value, index, array) {
	return value > 75; 
}

</script>



